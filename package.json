{
  "name": "@nadimtuhin/ytranscript",
  "version": "1.2.4",
  "description": "Fast YouTube transcript extraction with bulk processing, Google Takeout support, MCP server, and multiple output formats",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": "dist/cli.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && bun build ./src/cli.ts --outdir ./dist --target node && bun build ./src/mcp.ts --outdir ./dist --target node && tsc --emitDeclarationOnly --declaration --outDir ./dist && chmod +x dist/cli.js dist/mcp.js",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir ./dist",
    "dev": "bun run --watch src/cli.ts",
    "dev:mcp": "bun run --watch src/mcp.ts",
    "test": "bun test",
    "lint": "bunx @biomejs/biome check --write ./src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "youtube",
    "transcript",
    "captions",
    "subtitles",
    "bulk",
    "google-takeout",
    "cli",
    "mcp",
    "model-context-protocol",
    "ai",
    "bun"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nadimtuhin/ytranscript.git"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@types/bun": "^1.1.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.2",
    "commander": "^12.1.0",
    "linkedom": "^0.18.0",
    "p-limit": "^6.1.0",
    "undici": "^7.18.2",
    "zod": "^4.3.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  }
}
